Coral-RestaurantInterface-Agent/.env.example
############################################################
# Coral – Restaurant Interface Agent
############################################################

# --- Coral Server (MCP) connection ---
CORAL_SSE_URL=https://your-coral-server.example.com/sse

# Unique identifier (defaults to “restaurant-interface-agent” if unset)
CORAL_RESTAURANT_INTERFACE_AGENT_ID=restaurant-interface-agent

# --- Model settings ---
MODEL_NAME=gpt-4.1
MODEL_PROVIDER=openai        # openai | groq | other
API_KEY=your-llm-api-key
MODEL_TEMPERATURE=0.1
MODEL_TOKEN=8000             # max tokens

# --- Orchestration mode ---
CORAL_ORCHESTRATION_RUNTIME=devmode   # devmode | docker

# Used only when CORAL_ORCHESTRATION_RUNTIME=docker
HUMAN_RESPONSE=